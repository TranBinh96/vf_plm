        <value>ScheduleTask.work_estimate=Work</value>
        <value>ScheduleTask.work_complete=ActualWork</value>
        <value>ScheduleTask.fixed_type=Type</value>
        <value>ScheduleTask.complete_percent=PercentComplete</value>
