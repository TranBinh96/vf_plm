<?xml version="1.0" encoding="UTF-8"?>
               <preferences version="10.0">
               
  <category name="Substance Compliance">
    <category_description>Teamcenter Preferences for Substance Compliance</category_description>
    
               <preference name="SUBSCMPL_cmd_default_xml_file" type="String" array="false" disabled="false" protectionScope="Site" envEnabled="false">
      <preference_description>Specifies the path to the default IPC XML file name for material substance declaration requests.
The valid values are:
* IPC_1755.xml: IPC 1755 format XML template. This is the default value.</preference_description>
      <context name="Teamcenter">
        <value platform="UNX">$TC_DATA/subscmpl_data/sample/IPC_1755.xml</value>
        <value platform="WNT">%TC_DATA%\subscmpl_data\sample\IPC_1755.xml</value>
      </context>
    </preference>
    
               </category>
               </preferences>
               