<!-- Default style sheet for displaying TCCalendar summary in Active Workspace Client. -->
<rendering xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="XMLRenderingStylesheet_Schema.xsd">
    <header>
        <image source = "type"/>
    </header>
    <page titleKey="tc_xrt_Overview">
        <column>
            <section titleKey="tc_xrt_properties">
                <property name="tccal_name" />
                <property name="tccal_type" />
                <property name="tccal_desc" />
                <break/>
                <property name="time_zone"/>
                <break/>
                <command titleKey = "tc_xrt_moreProperties" commandId = "com.teamcenter.rac.properties"/>
            </section>
        </column>
    </page>
    <inject type="dataset" src="AuditLogForOrg"/>
</rendering>
