<subRendering> 
 <page titleKey="scw0_xrt_substanceCategories" title="Substance Categories"  visibleWhen="Scp0UsesSubstanceCategory!=null" >
            <section titleKey="scw0_xrt_substanceCategories" title="Substance Categories">
                <objectSet source="Scp0UsesSubstanceCategory.Scp0SubstanceCategory" defaultdisplay="tableDisplay" sortdirection="descending" sortby="Scp0UsesSubstanceCategory.scp0IsOverThreshold">
                    <tableDisplay>
                        <property name="scp0ParentListRef"/>
                        <property name="object_name" />
                        <property name="scp0ThresholdString" />
                        <property name="Scp0UsesSubstanceCategory.scp0IsOverThreshold"/>
                        <property name="Scp0UsesSubstanceCategory.scp0mass"/>
                        <property name="Scp0UsesSubstanceCategory.scp0uomString" />
                        <property name="Scp0UsesSubstanceCategory.scp0Concentration"/>
                        <property name="Scp0UsesSubstanceCategory.scp0Exemptions"/>
                    </tableDisplay>
                  </objectSet>
            </section>
    </page>
</subRendering> 