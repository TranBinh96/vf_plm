<!-- Default style sheet for displaying Substance Application Code Info Panel in Active Workspace Client. -->
<subRendering>
    <content visibleWhen="Scp0BOMSnapshot != null">
            <section titleKey="tc_supplier_part_Hierarchy">
                <objectSet source="Scp0BOMSnapshot.Scp0VPBOMItemRevision" defaultdisplay="tableDisplay" sortdirection="descending" sortby="object_name">
                    <tableDisplay>
                        <property name="scp0ProductId" modifiable="false"/>
                        <property name="object_name" modifiable="false"/>
                        <property name="scp0mass" modifiable="false"/>
                        <property name="scp0uom_tag" modifiable="false"/>
                    </tableDisplay>
                    <listDisplay/>
                </objectSet>
            </section>
    </content>
</subRendering>