		<property name="vf4_misce_piece_cost_target" column="16"/>
	</section>
	<separator/>		
	<section title="General Info">
		<property name="owning_group" column="16"/>
		<property name="last_mod_date" column="16"/>
		<property name="last_mod_user" column="16"/>
		<property name="checked_out_user" column="16"/>
	</section>
    </page>
  
</rendering>