	<section title="Target Cost">
		<label text="* Currency: USD"  style="font-size:15pt;font-style:plain;font-family:Tahoma;font-weight:bold;margin-left:50px"/>
		<property name="vf4_piece_cost_value_target"  column="16" />
		<property name="vf4_isPieceCostInParentPart"  column="16" renderingHint="checkbox"/>		
		<property name="vf4_ednd_cost_value_target" column="16" />
		<property name="vf4_isEDDCostInParentPart" column="16" renderingHint="checkbox"/>
		<property name="vf4_tooling_invest_target" column="16"/>
