<rendering>

    <page title="General" titleKey="tc_xrt_General">
        <column>
            <section titleKey="tc_xrt_References">
                <customPanel java="com.teamcenter.rac.contmgmtbase.wizards.RefContentPanelSkdCOM4"/>             
            </section>
            <section titleKey="tc_xrt_CommentaryDetails">
                <property name="revision:skdmodelic" />
                <property name="revision:skdsendid" />
                <property name="revision:skddiyear" />    
                <property name="revision:skdseqnum" />
                <property name="revision:skdctype" />
            </section>
            <separator/>
            <property name="object_name" renderingHint="textfield" />
            <property name="revision:DocumentTitle" renderingHint="textfield" />            
            <section titleKey="tc_xrt_OtherDetails">    
                <property name="revision:skdpriority" />
                <property name="revision:skdremarks" />     
                <property name="revision:skdresponse" />
                <property name="revision:skdsecurity_class" />            
            </section>
        </column>
        
        <column>
            <section titleKey="tc_xrt_DispatchTo">
                <section titleKey="tc_xrt_Contact">    
                    <property name="revision:skddispto_firstname" />
                    <property name="revision:skddispto_lastname" />
                    <property name="revision:skddispto_jobtitle" />
                    <property name="revision:skddispto_email" />
                    <property name="revision:skddispto_phone" />
                    <property name="revision:skddispto_fax" />
                    <property name="revision:skddispto_internet" />
                    <property name="revision:skddispto_dept" />
                    <property name="revision:skddispto_building" />
                    <property name="revision:skddispto_room" />
                </section>
                <section titleKey="tc_xrt_Address">    
                    <property name="revision:skddispto_street" />
                    <property name="revision:skddispto_pobox" />
                    <property name="revision:skddispto_city" />
                    <property name="revision:skddispto_state" />
                    <property name="revision:skddispto_zip" />
                    <property name="revision:skddispto_province" />
                    <property name="revision:skddispto_postcode" />
                    <property name="revision:skddispto_country" />
                </section>
            </section>        
      </column>      
      <customPanel java="com.teamcenter.rac.contmgmts1000d40.wizards.LanguagePanel"/>  
      <separator/>  
      <customPanel java="com.teamcenter.rac.contmgmtbase.wizards.RelationSelectionPanel"/>        
   </page>
   
