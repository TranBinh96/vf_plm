<!--
 Default style sheet for displaying Form properties. 
 type=form displays only properties that are available directly on the form
 type=property displays all properties
-->
<rendering xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="XMLRenderingStylesheet_Schema.xsd">
    <page>
        <all type="form"/>
    </page>
</rendering>