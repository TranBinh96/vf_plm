<rendering>
    
    <page title="Summary" titleKey="tc_xrt_Summary">
         <section titleKey="tc_xrt_properties">
            <property name="object_name" />
            <property name="object_desc" />
            <property name="object_type" />
            <separator/>
            <property name="owning_user" renderingHint="objectlink" modifiable="false" />
            <property name="owning_group" renderingHint="objectlink" modifiable="false" />
            <property name="last_mod_user" />
         </section>
         <section titleKey="tc_xrt_actions" commandLayout="vertical">
            <command actionKey="copyAction" commandId="com.teamcenter.rac.copy" titleKey="tc_xrt_Copy" />
            <command actionKey="propertiesAction" commandId="com.teamcenter.rac.properties" title="Properties..." />
         </section>
    </page>
</rendering>

