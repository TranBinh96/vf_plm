<rendering>
    <page title="Summary" titleKey="tc_xrt_Summary">
        <section titleKey="tc_xrt_properties">
            <property name="fnd0_final_val_result" modifiable="false" />
            <property name="validation_is_up_to_date" modifiable="false" />
            <property name="validation_data_obj" modifiable="false" />
            <property name="validation_target" modifiable="false" />
            <separator/>
            <property name="validation_user" modifiable="false" />
            <property name="validation_report"  modifiable="false" />
        </section>
    </page>
    
    <page title="Override Information" titleKey="tc_xrt_OverrideInformation">
        <section name="properties">
            <property name="override_request_user"  modifiable="false" />
            <property name="override_request_date"  modifiable="false" />
            <property name="override_type" modifiable="false" />
            <property name="override_to_state" modifiable="false" />
            <property name="override_category" modifiable="false" />
            <property name="override_reason" modifiable="false" />
            <property name="override_reason_detail"  modifiable="false" />
            <separator/>
            <property name="override_decision_user" modifiable="false" />
            <property name="override_decision_date"  modifiable="false" />
            <property name="override_request_state" modifiable="false" />
            <property name="override_decision_comments" modifiable="false" />
        </section>
    </page>

    <page title="All" titleKey="All">
        <all type="property"/>
    </page>
</rendering>
