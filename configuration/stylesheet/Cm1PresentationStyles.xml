<Style>

  <EdgePresentation id="StructureRelationStyle">
    <parameter name="shape">
      <value>BoxInBox</value>
    </parameter>
  </EdgePresentation>
  <EdgePresentation id="StructureRelationStyleWithEdge">
    <parameter name="shape">
      <value>Polyline</value>
    </parameter>
    <parameter name="lineWidth">
      <value>2</value>
    </parameter>
    <parameter name="arrowOption">
      <value>target</value>
    </parameter>
    <parameter name="arrowTargetShape">
      <value>triangle</value>
    </parameter>
    <parameter name="arrowTargetScale">
      <value>2.0</value>
    </parameter>
  </EdgePresentation>
  <NodePresentation id="DefaultNodeStyle">
    <parameter name="shape">
      <value>Tile</value>
    </parameter>
  </NodePresentation>
  
</Style>
